int i111() { return 111; }
int i888() { return 888; }

void test1() {
	printString("\ntest1");
	int x = i111(), y = i888();
	printInt(x + y * (x - y));
	if (x > y) {
		printInt(x + y * (x - y));
	} else {
		printInt(y * (x - y) + x);
		printInt(y + y * (x - y));
	}
	printInt(x + y * (x - y));
}

